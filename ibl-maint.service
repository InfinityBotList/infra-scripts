[Service]
Type=simple
Description="Maint service"
After=network.target
StartLimitIntervalSec=0
ExecStart=/root/void/void
User=root
Group=root
RuntimeDirectory=/root
WorkingDirectory=/root
ExecReload=/bin/kill -s HUP $MAINPID
KillMode=mixed
TimeoutStopSec=5
PrivateTmp=true
RestartSec=1
Restart=always

[Install]
WantedBy=multi-user.target
